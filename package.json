{
  "name": "bot-wandering-in-street-view",
  "version": "1.0.0",
  "description": "A bot wander aimlessly into the optic nerve of google street map",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean transpile start-app",
    "start-app": "taiko --observe ./lib/index.js",
    "transpile": "babel ./src -d ./lib ",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodadoa/bot-wandering-in-street-view.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dodadoa/bot-wandering-in-street-view/issues"
  },
  "homepage": "https://github.com/dodadoa/bot-wandering-in-street-view#readme",
  "dependencies": {
    "taiko": "^1.0.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env", 
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ]
  }
}
